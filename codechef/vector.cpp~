#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main()
{
	vector<int> v;

	v.push_back(4);
	v.push_back(3);

	// cout << vector<int>::iterator v.begin();


	sort(v.begin(), v.end(), std::greater<int>);
	for (vector<int>::iterator i = v.begin(); i < v.end(); ++i)
		cout << *i << " ";
	cout << endl;

	
return 0;
}
